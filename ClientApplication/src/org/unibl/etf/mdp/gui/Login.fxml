	fx:controller="org.unibl.etf.mdp.gui.LoginController">
	<children>
		<TextField fx:id="usernameTF" focusTraversable="false"
			layoutX="87.0" layoutY="154.0" prefHeight="37.0" prefWidth="225.0"
			promptText="Username" styleClass="text-field" />
		<Button fx:id="logInButton" layoutX="244.0" layoutY="270.0"
			mnemonicParsing="false" onAction="#logIn" text="Log In" />
		<ImageView fx:id="image" fitHeight="50.0" fitWidth="50.0"
			layoutX="175.0" layoutY="61.0" pickOnBounds="true"
			preserveRatio="true" />
		<PasswordField fx:id="passwordTF" layoutX="87.0"
			layoutY="214.0" prefHeight="37.0" prefWidth="225.0"
			promptText="Password" />
	</children>
